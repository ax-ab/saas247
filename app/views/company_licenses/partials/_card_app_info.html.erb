  <div class="big-metrics-card">
    <div class="metrics-header">
      <div class="header-content">
        <h4><%= cardName %></h4>
        <p>Last 12 months</p>
      </div>
      <a href="">View All</a>
    </div>
    <% apps.each do | app | %>
    <div class="app-info">

      <%= image_tag("#{app.logo}", class: "app-logo") %>

      <div class="app-name"><%= app.key %></div>
      <div class="app-expense"><%= number_to_currency(app.expense, unit: "â‚¬ ", separator: ",", delimiter: ".", precision: 0) %></div>
    </div>
    <% end %>
  </div>
