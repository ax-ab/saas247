        <h4><%= cardName %></h4>
        <p>Last 12 months</p>
      </div>
      <%= link_to 'View All', company_licenses_path(sort: 'expense') %>
    </div>
    <% apps.each do | app | %>
    <div class="app-info">

      <%= image_tag("#{app.logo}", class: "app-logo") %>

      <div class="app-name">
      <%= link_to company_license_path(app.id) do %>
        <%= app.key %>
              <% end %></div>

      <div class="app-expense"><%= number_to_currency(app.expense, unit: "â‚¬ ", separator: ",", delimiter: ".", precision: 0) %></div>


    </div>
    <% end %>
  </div>

